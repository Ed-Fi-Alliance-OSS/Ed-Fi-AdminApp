import { I<%= className %> } from "../interfaces/<%= fileName %>.interface";
import { PostDto, DtoPostBase } from "../utils/dto-post-base";

export class Get<%= className %>Dto extends DtoGetBase implements GetDto<I<%= className %>> {
  @Expose()
  name: string;

  override get displayName() {
    return this.name
  }
}
export const toGet<%= className %>Dto = makeSerializer(Get<%= className %>Dto)

export class Put<%= className %>Dto extends DtoPutBase implements PutDto<I<%= className %>> {
  @Expose()
  @IsString()
